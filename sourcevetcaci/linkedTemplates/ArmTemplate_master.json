{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sourcevetcaci"
		},
		"Oracle1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Oracle1'"
		},
		"Oracle1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "servername"
		},
		"Oracle1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "vera"
		},
		"AzureBlobStorage529_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/a46f305f-dbca-4bd1-871e-bbd489fc06e9/resourceGroups/Synapse/providers/Microsoft.Storage/storageAccounts/aaaa1234"
		},
		"AzureBlobStorage529_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"default_properties_param1_value": {
			"type": "string",
			"defaultValue": "asdfg"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sourcevetcaci_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Oracle1_password": {
						"value": "[parameters('Oracle1_password')]"
					},
					"default_properties_param1_value": {
						"value": "[parameters('default_properties_param1_value')]"
					},
					"Oracle1_properties_typeProperties_server": {
						"value": "[parameters('Oracle1_properties_typeProperties_server')]"
					},
					"Oracle1_properties_typeProperties_username": {
						"value": "[parameters('Oracle1_properties_typeProperties_username')]"
					},
					"AzureBlobStorage529_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStorage529_properties_privateLinkResourceId')]"
					},
					"AzureBlobStorage529_properties_groupId": {
						"value": "[parameters('AzureBlobStorage529_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}